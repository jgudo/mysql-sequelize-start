{
  "name": "mysql-sequelize-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Julius Guevarra",
  "scripts": {
    "start": "nodemon --exec ts-node --files -r tsconfig-paths/register src/server.ts",
    "build": "rimraf ./build && tsc && ef-tspm",
    "dev": "node dist/index.js",
    "build-ts": "tsc",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/sequelize": "^4.28.9",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "sequelize-cli-typescript": "^3.2.0-c",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
